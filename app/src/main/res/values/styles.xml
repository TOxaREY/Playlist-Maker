<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="LinearLayoutAttr">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="FrameLayoutAttr">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/header_layout_height</item>
        <item name="android:layout_marginTop">@dimen/header_layout_marginTop</item>
        <item name="android:layout_marginStart">@dimen/header_layout_marginStart</item>
        <item name="android:layout_marginEnd">@dimen/header_layout_marginEnd</item>
    </style>
    <style name="ButtonsAttr" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/main_button_layout_height</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_marginTop">@dimen/buttons_layout_marginTop</item>
        <item name="android:backgroundTint">@color/yp_white</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textColor">@color/yp_black</item>
        <item name="android:textSize">@dimen/button_textSize</item>
        <item name="android:insetTop">@dimen/button_insetTop</item>
        <item name="android:insetBottom">@dimen/button_insetBottom</item>
        <item name="iconGravity">textStart</item>
        <item name="iconPadding">@dimen/main_button_iconPadding</item>
        <item name="iconTint">@color/yp_black</item>
        <item name="cornerRadius">@dimen/button_cornerRadius</item>
    </style>
    <style name="FrameLayoutSettings">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/settings_menu_layout_height</item>
    </style>
    <style name="TextViewAttr">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/padding_and_margin_horizontal</item>
        <item name="android:layout_marginTop">@dimen/settings_menu_layout_marginTop</item>
        <item name="android:textColor">?attr/colorPrimary</item>
        <item name="android:textSize">@dimen/settings_menu_textSize</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
    </style>
    <style name="TextViewTitleAttr" parent="TextViewAttr">
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textSize">@dimen/title_textSize</item>
        <item name="android:layout_marginTop">@dimen/title_layout_marginTop</item>
        <item name="android:layout_marginStart">12dp</item>
        <item name="android:textColor">?attr/colorPrimary</item>
    </style>
    <style name="ImageViewAttr">
        <item name="android:layout_width">@dimen/settings_menu_imageView_layout_width</item>
        <item name="android:layout_height">@dimen/settings_menu_imageView_layout_height</item>
        <item name="android:layout_marginTop">@dimen/settings_menu_imageView_layout_marginTop</item>
        <item name="android:layout_marginEnd">@dimen/settings_menu_imageView_layout_marginEnd</item>
        <item name="android:layout_gravity">end</item>
        <item name="android:state_selected">true</item>
    </style>
    <style name="BackButtonAttr" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="android:layout_width">48dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:backgroundTint">?attr/backgroundColor</item>
        <item name="android:insetTop">@dimen/button_insetTop</item>
        <item name="android:insetBottom">@dimen/button_insetBottom</item>
        <item name="icon">@drawable/ic_arrow_back</item>
        <item name="iconGravity">textStart</item>
        <item name="iconPadding">0dp</item>
        <item name="iconSize">16dp</item>
        <item name="iconTint">?attr/colorPrimary</item>
    </style>
    <style name="ClearButtonAttr" parent="Widget.MaterialComponents.Button.UnelevatedButton">
    </style>
    <style name="TextViewTrackAttr">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/track_item_textView_and_dot_image_layout_marginTop</item>
        <item name="android:layout_marginBottom">@dimen/track_item_textView_and_dot_image_layout_marginBottom</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textSize">11sp</item>
        <item name="android:textColor">?attr/itemFillColor</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
    </style>
    <style name="LinearLayoutTrackAttr">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="UpdateAndClearAndNewPlaulistButtonAttr" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">36dp</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:backgroundTint">?attr/colorPrimary</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:insetTop">@dimen/button_insetTop</item>
        <item name="android:insetBottom">@dimen/button_insetBottom</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
        <item name="android:textSize">14sp</item>
        <item name="cornerRadius">54dp</item>
    </style>
    <style name="TextViewErrorAttr">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_marginTop">238dp</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textColor">?attr/colorPrimary</item>
        <item name="android:textSize">19sp</item>
        <item name="android:visibility">invisible</item>
    </style>
    <style name="ImageViewErrorAttr">
        <item name="android:layout_width">120dp</item>
        <item name="android:layout_height">120dp</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_marginTop">102dp</item>
        <item name="android:visibility">invisible</item>
    </style>
    <style name="FrameLayoutSearchAndMediaLibraryAttr">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="RecyclerViewSearchAttr">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="layoutManager">androidx.recyclerview.widget.LinearLayoutManager</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="LinearLayoutHistoryAttr">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="ConstraintLayoutAttr">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">?attr/backgroundColor</item>
    </style>
    <style name="GuidelineAttr">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="TextViewInfoAttr">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">17dp</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">?attr/itemFillColor</item>
        <item name="layout_constraintStart_toStartOf">@+id/leftBorderInfo</item>
    </style>
    <style name="TextViewInfoValueAttr">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:layout_marginBottom">17dp</item>
        <item name="android:ellipsize">end</item>
        <item name="android:text">@string/placeholder</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:maxLines">1</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">?attr/colorPrimary</item>
        <item name="layout_constrainedWidth">true</item>
        <item name="layout_constraintHorizontal_bias">1</item>
        <item name="layout_constraintStart_toEndOf">@+id/titleBarrier</item>
    </style>
    <style name="PlayerButtonAttr">
        <item name="android:layout_width">@dimen/player_addbutton_and_likebutton_width_height_fabCustomSize</item>
        <item name="android:layout_height">@dimen/player_addbutton_and_likebutton_width_height_fabCustomSize</item>
        <item name="fabCustomSize">@dimen/player_addbutton_and_likebutton_width_height_fabCustomSize</item>
        <item name="android:layout_marginBottom">84dp</item>
        <item name="android:backgroundTint">?attr/colorBackgroundFloating</item>
        <item name="android:foregroundGravity">center</item>
        <item name="borderWidth">0dp</item>
        <item name="elevation">0dp</item>
        <item name="layout_constraintBottom_toTopOf">@+id/tvDuration</item>
        <item name="maxImageSize">23dp</item>
        <item name="tint">@color/yp_white</item>
    </style>
    <style name="TextViewInfoTitleAttr">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:ellipsize">end</item>
        <item name="android:text">@string/placeholder</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:maxLines">1</item>
        <item name="android:textColor">?attr/colorPrimary</item>
        <item name="layout_constrainedWidth">true</item>
        <item name="layout_constraintHorizontal_bias">0</item>
        <item name="layout_constraintEnd_toEndOf">@+id/rightBorderPlayer</item>
        <item name="layout_constraintStart_toStartOf">@+id/leftBorderPlayer</item>
    </style>
    <style name="TabLayoutAttr">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/tab_layout_marginLeft_and_marginRight</item>
        <item name="android:layout_marginRight">@dimen/tab_layout_marginLeft_and_marginRight</item>
        <item name="tabTextColor">?attr/colorPrimary</item>
        <item name="tabSelectedTextColor">?attr/colorPrimary</item>
        <item name="android:background">?attr/backgroundColor</item>
        <item name="tabIndicatorColor">?attr/colorPrimary</item>
    </style>
    <style name="TabLayoutTextAppearanceAttr" parent="TextAppearance.Design.Tab">
        <item name="textAllCaps">false</item>
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
    </style>
    <style name="ViewPagerAttr">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="ConstraintLayoutRootAttr">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="FragmentContainerViewRootAttr">
        <item name="android:layout_width">@dimen/activity_root_layout_width_and_height_and_bottomNavigationView_layout_width</item>
        <item name="android:layout_height">@dimen/activity_root_layout_width_and_height_and_bottomNavigationView_layout_width</item>
        <item name="layout_constraintBottom_toTopOf">@id/bottomNavigationView</item>
        <item name="layout_constraintEnd_toEndOf">parent</item>
        <item name="layout_constraintStart_toStartOf">parent</item>
        <item name="layout_constraintTop_toTopOf">parent</item>
    </style>
    <style name="BottomNavigationViewAttr">
        <item name="android:layout_width">@dimen/activity_root_layout_width_and_height_and_bottomNavigationView_layout_width</item>
        <item name="android:layout_height">56dp</item>
        <item name="layout_constraintBottom_toBottomOf">parent</item>
        <item name="layout_constraintEnd_toEndOf">parent</item>
        <item name="layout_constraintStart_toStartOf">parent</item>
        <item name="itemIconTint">@drawable/nav_item_color_state</item>
        <item name="itemTextColor">@drawable/nav_item_color_state</item>
        <item name="itemBackground">?attr/backgroundColor</item>
    </style>
</resources>