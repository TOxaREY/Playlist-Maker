<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="312dp"
        android:height="312dp"
        android:gravity="center">

        <shape
            android:shape="rectangle">
            <stroke
                android:color="@color/yp_text_gray"
                android:width="1dp"
                android:dashWidth="@dimen/new_playlist_add_photo_background_dash"
                android:dashGap="@dimen/new_playlist_add_photo_background_dash"/>
            <corners
                android:radius="8dp"/>
        </shape>
    </item>

    <item
        android:width="100dp"
        android:height="100dp"
        android:gravity="center"
        android:drawable="@drawable/ic_add_photo">
    </item>
</layer-list>